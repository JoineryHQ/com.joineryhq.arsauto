<?xml version="1.0"?>
<extension key="com.joineryhq.arsauto" type="module">
  <file>arsauto</file>
  <name>ARS auto</name>
  <description>Performs automatic calculation of certain data points for ARS.</description>
  <license>GPL-3.0</license>
  <authors>
    <author>
      <name>Allen Shaw</name>
      <email>allen@JoineryHQ.com</email>
      <role>Maintainer</role>
    </author>
  </authors>
  <urls>
    <url desc="Main Extension Page">https://github.com/JoineryHQ/com.joineryhq.arsauto</url>
    <url desc="Documentation">https://github.com/JoineryHQ/com.joineryhq.arsauto/blob/master/README.md</url>
    <url desc="Support">https://github.com/JoineryHQ/com.joineryhq.arsauto/issues</url>
    <url desc="Licensing">https://www.gnu.org/licenses/gpl-3.0.html</url>
  </urls>
  <releaseDate>2025-12-13</releaseDate>
  <version>1.0.0</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>6.2</ver>
  </compatibility>
  <comments>Provides several managed entities which are forced to be enabled whenever this extension is enabled; see Documentation.</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Arsauto</namespace>
    <format>25.01.1</format>
    <angularModule>crmArsauto</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty@1.0.3</mixin>
  </mixins>
  <upgrader>CiviMix\Schema\Arsauto\AutomaticUpgrader</upgrader>
</extension>
